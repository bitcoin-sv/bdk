  "version": "0.2.0",
  "configurations": [
    {
      "name": "CMD Main",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/main.go",
      "cwd": "${workspaceFolder}",
      "buildFlags": "",
      "env": {"LD_LIBRARY_PATH":"${env:LOCAL_BDK_INSTALL}/INSTALLATION/bin"
        ,"CGO_LDFLAGS":"-L${env:LOCAL_BDK_INSTALL}/INSTALLATION/bin"
        ,"CGO_CFLAGS":"-I${env:LOCAL_BDK_INSTALL}/INSTALLATION/include/cgo"
        ,"GOBDK_SCRIPTCONFIG_MAXOPSPERSCRIPTPOLICY":"1"
        ,"GOBDK_SCRIPTCONFIG_MAXSCRIPTNUMLENGTHPOLICY":"2"
        ,"GOBDK_SCRIPTCONFIG_MAXSCRIPTSIZEPOLICY":"3"
        ,"GOBDK_SCRIPTCONFIG_MAXPUBKEYSPERMULTISIG":"4"
        ,"GOBDK_SCRIPTCONFIG_MAXSTACKMEMORYUSAGECONSENSUS":"5"
        ,"GOBDK_SCRIPTCONFIG_MAXSTACKMEMORYUSAGEPOLICY":"6"
      },
      "showLog": true
    }
  ]
}